{
  "name": "saju-astro-chatbot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "node scripts/bootstrap-python.mjs",
    "postinstall": "prisma generate",
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "typecheck": "tsc -p tsconfig.json",
    "typecheck:watch": "tsc -p tsconfig.json --watch --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run tests/integration",
    "test:e2e:api": "vitest run tests/e2e",
    "test:unit": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\",\"jsx\":\"react-jsx\"}&& node -r ts-node/register --test tests/*.test.ts",
    "test:backend": "python -m pytest backend_ai/tests",
    "dev:astro": "ts-node -P tsconfig.scripts.json -r tsconfig-paths/register scripts/dev-astro.ts",
    "check:env": "node scripts/validate-env.js",
    "check:all": "npm run lint && npm run typecheck && npm test",
    "quality:check": "npm run lint && npm run typecheck && npm run test:coverage",
    "quality:report": "npm run test:coverage && npm run lint -- --format html --output-file lint-report.html",
    "migrate:logger": "node scripts/migrate-console-to-logger.js",
    "marketing:auto-post": "node scripts/auto-post-daily-fortune.mjs",
    "marketing:test": "node scripts/auto-post-daily-fortune.mjs --test",
    "mobile:sync": "npm run build && npx cap sync",
    "mobile:sync:android": "npm run build && npx cap sync android",
    "mobile:sync:ios": "npm run build && npx cap sync ios",
    "mobile:open:android": "npx cap open android",
    "mobile:open:ios": "npx cap open ios",
    "mobile:build": "npm run build && npx cap sync && npx cap open android"
  },
  "overrides": {
    "node-gyp": "^10.0.1",
    "glob": "^10.4.7",
    "swisseph": {
      "node-gyp": "^10.0.1"
    }
  },
  "dependencies": {
    "@aharris02/bazi-calculator-by-alvamind": "^1.0.16",
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^7.2.0",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/nextjs": "^10.30.0",
    "@vercel/speed-insights": "^1.2.0",
    "astronomia": "^4.2.0",
    "astronomy-engine": "^2.1.19",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.5.1",
    "circular-natal-horoscope-js": "^1.1.0",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.18",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.2",
    "firebase": "^12.6.0",
    "framer-motion": "^12.23.22",
    "google-auth-library": "^10.4.0",
    "korean-lunar-calendar": "^0.3.6",
    "next": "^16.0.10",
    "next-auth": "^4.24.13",
    "next-pwa": "^5.6.0",
    "nodemailer": "^7.0.11",
    "numerology": "^1.0.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.4.168",
    "pg": "^8.16.3",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.2",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.2",
    "react-markdown": "^10.1.0",
    "replicate": "^1.3.0",
    "resend": "^6.5.2",
    "stripe": "^19.3.0",
    "swisseph": "^0.5.17",
    "three": "^0.181.1",
    "tz-lookup": "^6.1.25",
    "web-push": "^3.6.7",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.19.24",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.181.0",
    "@types/tz-lookup": "^6.1.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "^16.0.10",
    "happy-dom": "^20.0.11",
    "postcss": "^8.5.6",
    "prisma": "^7.2.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^4.0.16",
    "webpack": "^5.102.1"
  }
}
