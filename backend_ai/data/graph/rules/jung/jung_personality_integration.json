  "cross_validation": {
    "description": "Aura + 사주 + 점성술 교차 검증",
    "validation_rules": {
      "energy_axis": {
        "aura_radiant_saju_check": ["식상 강도", "재성 강도", "비겁 약세"],
        "aura_grounded_saju_check": ["인성 강도", "비겁 강도", "식상 약세"],
        "aura_radiant_astro_check": ["태양/화성 강조", "1/5하우스", "불/바람 사인"],
        "aura_grounded_astro_check": ["달/토성 강조", "4/12하우스", "물/땅 사인"]
      },
      "congruence_scoring": {
        "all_match": {
          "score": "A",
          "interpretation": "타고난 성향이 명확. 자기 이해 정확.",
          "action": "강점 더 활용"
        },
        "two_match": {
          "score": "B",
          "interpretation": "주요 성향 일치. 한 소스에서 보완점.",
          "action": "불일치 소스 탐색 - 발달 or 억압"
        },
        "no_match": {
          "score": "C",
          "interpretation": "복잡한 내면. 환경 적응 강함.",
          "action": "어떤 것이 '진짜 나'인지 탐색 필요"
        }
      }
    },
    "conflict_insights": {
      "aura_radiant_saju_grounded": {
        "pattern": "외향 테스트 결과 but 내향 사주",
