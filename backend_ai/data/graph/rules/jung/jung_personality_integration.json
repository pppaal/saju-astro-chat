{
  "$schema": "jung_personality_integration_v1",
  "$description": "Aura Quiz 결과와 사주/점성술/타로/융 통합 분석 구조",

  "aura_quiz_schema": {
    "description": "Nova Persona Aura Quiz 결과 형식",
    "axes": {
      "energy": {
        "poles": ["radiant", "grounded"],
        "letters": {"radiant": "R", "grounded": "G"},
        "description": "에너지 방향: 외부 발산(R) vs 내부 집중(G)",
        "mbti_parallel": "E/I"
      },
      "cognition": {
        "poles": ["visionary", "structured"],
        "letters": {"visionary": "V", "structured": "S"},
        "description": "인식 방식: 가능성/패턴(V) vs 구체/사실(S)",
        "mbti_parallel": "N/S"
      },
      "decision": {
        "poles": ["logic", "empathic"],
        "letters": {"logic": "L", "empathic": "H"},
        "description": "판단 기준: 논리/객관(L) vs 공감/가치(H)",
        "mbti_parallel": "T/F"
      },
      "rhythm": {
        "poles": ["flow", "anchor"],
        "letters": {"flow": "F", "anchor": "A"},
        "description": "생활 리듬: 유연/즉흥(F) vs 계획/구조(A)",
        "mbti_parallel": "P/J"
      }
    },
    "input_format": {
      "typeCode": "RVLA",
      "personaName": "Starforge Navigator",
      "axes": {
        "energy": {"pole": "radiant", "score": 72},
        "cognition": {"pole": "visionary", "score": 85},
        "decision": {"pole": "logic", "score": 65},
        "rhythm": {"pole": "anchor", "score": 58}
      },
      "profile": {
        "openness": 85,
        "conscientiousness": 58,
        "extraversion": 72,
        "agreeableness": 35,
        "neuroticism": 50,
        "introversion": 28,
        "intuition": 85,
        "thinking": 65,
        "perceiving": 42
      },
      "consistencyScore": 75
    }
  },

  "aura_to_cognitive_functions": {
    "description": "Aura 축 조합 → 8가지 인지기능 추론",
    "mapping_rules": {
      "dominant_function": {
        "RV": {"high_score": "Ne", "description": "외향 직관 - 가능성 탐색"},
        "GV": {"high_score": "Ni", "description": "내향 직관 - 내면 통찰"},
        "RS": {"high_score": "Se", "description": "외향 감각 - 현재 순간"},
        "GS": {"high_score": "Si", "description": "내향 감각 - 경험 축적"},
        "RL": {"high_score": "Te", "description": "외향 사고 - 체계적 조직"},
        "GL": {"high_score": "Ti", "description": "내향 사고 - 논리 분석"},
        "RH": {"high_score": "Fe", "description": "외향 감정 - 관계 조화"},
        "GH": {"high_score": "Fi", "description": "내향 감정 - 내면 가치"}
      },
      "function_stack_inference": {
        "RVLF": {"stack": ["Ne", "Ti", "Fe", "Si"], "similar_mbti": "ENTP"},
        "RVLA": {"stack": ["Ne", "Te", "Fi", "Si"], "similar_mbti": "ENTJ-Ne"},
        "RVHF": {"stack": ["Ne", "Fi", "Te", "Si"], "similar_mbti": "ENFP"},
        "RVHA": {"stack": ["Ne", "Fe", "Ti", "Si"], "similar_mbti": "ENFJ-Ne"},
        "GVLF": {"stack": ["Ni", "Ti", "Fe", "Se"], "similar_mbti": "INTP-Ni"},
        "GVLA": {"stack": ["Ni", "Te", "Fi", "Se"], "similar_mbti": "INTJ"},
        "GVHF": {"stack": ["Ni", "Fi", "Te", "Se"], "similar_mbti": "INFP-Ni"},
        "GVHA": {"stack": ["Ni", "Fe", "Ti", "Se"], "similar_mbti": "INFJ"},
        "RSLF": {"stack": ["Se", "Ti", "Fe", "Ni"], "similar_mbti": "ESTP"},
        "RSLA": {"stack": ["Se", "Te", "Fi", "Ni"], "similar_mbti": "ESTJ-Se"},
        "RSHF": {"stack": ["Se", "Fi", "Te", "Ni"], "similar_mbti": "ESFP"},
        "RSHA": {"stack": ["Se", "Fe", "Ti", "Ni"], "similar_mbti": "ESFJ-Se"},
        "GSLF": {"stack": ["Si", "Ti", "Fe", "Ne"], "similar_mbti": "ISTP-Si"},
        "GSLA": {"stack": ["Si", "Te", "Fi", "Ne"], "similar_mbti": "ISTJ"},
        "GSHF": {"stack": ["Si", "Fi", "Te", "Ne"], "similar_mbti": "ISFP-Si"},
        "GSHA": {"stack": ["Si", "Fe", "Ti", "Ne"], "similar_mbti": "ISFJ"}
      }
    }
  },

