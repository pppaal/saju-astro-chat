      "multimodal_engine": {
        "path": "rules/integration/multimodal_integration_engine.json",
        "provides": [
          "correlation_matrix - 사주↔점성술↔타로 상관관계",
          "question_router - 질문 유형별 분석 소스 선택",
          "synthesis_template - 통합 응답 템플릿",
          "conflict_resolution - 소스 간 충돌 처리"
        ],
        "usage": "멀티모달 통합 분석 시 엔진으로 활용"
      },
      "jung_cross_analysis": {
        "path": "rules/jung/jung_cross_analysis.json",
        "provides": [
          "sipsin_archetype_map - 십신↔융 원형 매핑",
          "planet_archetype_map - 행성↔융 원형 매핑",
          "integrated_shadow_work - 통합 그림자 작업"
        ],
        "usage": "융 심리학적 해석 레이어 적용"
      }
    },
    "integration_flow": {
      "step_1": {
        "action": "Aura Quiz 결과 수신",
        "data": "typeCode, axes scores, profile"
      },
      "step_2": {
        "action": "기존 교차 규칙으로 사주/점성 데이터 매핑",
        "source": "compatibility.json의 element_fusion 활용",
        "mapping": {
          "aura_energy_radiant": "compatibility의 fire/air 요소 강조",
          "aura_energy_grounded": "compatibility의 earth/water 요소 강조",
