  "api_integration": {
    "description": "기존 /api/personality와 연동",
    "data_flow": {
      "step_1": "사용자가 Aura Quiz 완료 → /api/personality POST",
      "step_2": "저장된 데이터: typeCode, personaName, axes scores, profile",
      "step_3": "통합 분석 요청 시 사주/점성술 데이터와 조합",
      "step_4": "이 파일의 매핑 규칙 적용하여 통합 프로필 생성"
    },
    "prisma_schema_reference": {
      "model": "PersonalityResult",
      "fields": ["typeCode", "personaName", "energyScore", "cognitionScore", "decisionScore", "rhythmScore", "consistencyScore", "analysisData"]
    }
  },

  "existing_cross_references": {
    "description": "기존 사주↔점성술 교차 데이터 활용 - 이미 만들어진 규칙들",
